{
	"id": "biome",
	"icon": "mdi-island",
	"detect": {
		"packType": "behaviorPack",
		"scope": "biomes/",
		"matcher": ["biomes/**/*", "biomes/*"],
		"fileContent": ["minecraft:biome"],
		"fileExtensions": [".json"]
	},

	"schema": "file:///data/packages/minecraftBedrock/schema/biome/main.json",
	"lightningCache": "biome.json",

	"documentation": {
		"baseUrl": "https://bedrock.dev/docs/stable/Biomes",
		"supportsQuerying": false
	},
	"highlighterConfiguration": {
		"variables": ["description", "components"],
		"typeIdentifiers": ["format_version"]
	}
}
