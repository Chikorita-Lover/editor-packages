{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Volume",
	"type": "object",
	"required": ["format_version", "minecraft:volume"],
	"allOf": [
		{
			"properties": {
				"format_version": {
					"type": "string",
					"enum": ["1.17.0", "1.17.30"]
				}
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["", "1.17.0"]
					}
				}
			},
			"then": {
				"$ref": "./v1.17.0/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["1.17.30", "1.17.40", "1.18.0", "1.18.10"]
					}
				}
			},
			"then": {
				"$ref": "./v1.17.30/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["1.18.20", "1.18.30", "1.19.0", "1.19.10", "1.19.20"]
					}
				}
			},
			"then": {
				"$ref": "./v1.18.20/main.json"
			}
		}
	]
}
