{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Climate",
	"description": "Describes temperature, humidity, precipitation, etc.  Biomes without this component will have default values.",
	"additionalProperties": false,
	"type": "object",
	"properties": {
		"downfall": {
			"type": "number"
		},
		"snow_accumulation": {
			"type": "array",
			"items": {
				"type": "number"
			}
		},
		"temperature": {
			"type": "number"
		},
		"red_spores": {
			"type": "number"
		},
		"blue_spores": {
			"type": "number"
		},
		"ash": {
			"type": "number"
		},
		"white_ash": {
			"type": "number"
		}
	}
}
